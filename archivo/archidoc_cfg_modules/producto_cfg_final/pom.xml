<?xml version="1.0" encoding="UTF-8" standalone="no"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<parent>
		<groupId>es.ieci.tecdoc.archidoc.archidoc_cfg</groupId>
		<artifactId>archidoc_cfg_modules</artifactId>
		<version>5.0.9-M1</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<artifactId>producto_cfg_final</artifactId>
	<packaging>pom</packaging>
	<name>Configuracion Producto Final</name>
	<description>Configuracion Producto Final</description>
	<properties>
		<name>producto_cfg_final</name>
	</properties>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack-common-resources</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>es.ieci.tecdoc.archidoc.archidoc_cfg</groupId>
									<artifactId>producto_cfg_local</artifactId>
									<version>${project.version}</version>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory>
								${basedir}\target\config
							</outputDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<executions>
					<execution>
						<id>assembly-common-resources</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>
									${basedir}/../archidoc_cfg_main/src/main/assembly/dep-configuration-final.xml
								</descriptor>
							</descriptors>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>