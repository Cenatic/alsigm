<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Formateador para el listado de tramites asociados a la fase: Edicion de Fase -> Tramites asociados -->
<beanformatter>

	<propertyfmt type="BeanPropertySimpleFmt"
				 property='CTTASK:NOMBRE'
				 readOnly='false'
				 titleKey='catalog.common.nombre'
				 fieldType='DATA'
				 headerClass='sortable'
				 sortable='true'
				 columnClass='width30percent'
				 />
				 
	<propertyfmt type="BeanPropertySimpleFmt"
				 property='CTTASK:COD_TRAM'
				 readOnly='false'
				 titleKey='catalog.common.codigo'
				 fieldType='DATA'
				 headerClass='sortable'
				 sortable='true'
				 columnClass='width10percent'
				 />
	
	<propertyfmt type="BeanPropertySimpleFmt"
				 property='CTTASK:DESCRIPCION'
				 readOnly='false'
				 titleKey='catalog.common.descripcion'
				 fieldType='DATA'
				 headerClass='sortable'
				 sortable='true'
				 columnClass='width40percent'
				 />

</beanformatter>