<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Formateador para seleccionar tramites -->
<beanformatter>

	<propertyfmt type="BeanPropertySimpleFmt"
				 property='ID'
				 readOnly='false'
				 titleKey='empty'
				 fieldType='CHECKBOX'
				 headerClass='headerDisplay'
				 sortable='false'
				 media='html'
				 columnClass='width5percent'
				 url='/addCTStageTask.do'
				 />

	<propertyfmt type="BeanPropertySimpleFmt"
				 property='NOMBRE'
				 readOnly='false'
				 titleKey='catalog.common.nombre'
				 fieldType='SUBMIT'
				 fieldLink='ID'
				 styleClass='displayLink'
				 headerClass='sortable'
				 sortable='true'
				 media='html'
				 columnClass='width40percent'
				 url='/addCTStageTask.do'
				 id='taskId'
				 />
				 
	<propertyfmt type="BeanPropertySimpleFmt"
				 property='DESCRIPCION'
				 readOnly='false'
				 titleKey='catalog.common.descripcion'
				 fieldType='DATA'
				 headerClass='sortable'
				 sortable='true'
				 columnClass='width55percent'
				 />
				 
</beanformatter>