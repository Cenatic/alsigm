				 headerClass='headerDisplay'
				 columnClass="width15percent"
				 />
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='SUSTITUTO_ESTADOFIRMA' 
				 titleKey='formatter.signedDocuemnt.estadoFirma' 
				 fieldType='LIST' 
				 headerClass='headerDisplay'
				 columnClass="width15percent"
				 />

	<propertyfmt type="BeanPropertyDateFmt" 
				 property='FFIRMA'
				 format='dd/MM/yyyy'
				 titleKey='formatter.signedDocuemnt.fechaFirma' 
				 fieldType='LIST' 
				 headerClass='headerDisplay'
				 columnClass="width15percent" 
				 comparator='ieci.tdw.ispac.ispacweb.comparators.DateComparator'
				 />
				 
</beanformatter>
